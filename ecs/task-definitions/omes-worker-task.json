{
  "family": "omes-worker",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "4096",
  "executionRoleArn": "arn:aws:iam::<ACCOUNT_ID>:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::<ACCOUNT_ID>:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "sdk-worker",
      "image": "<ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/sdk-perf-tests-rep:go-worker-latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9092,
          "protocol": "tcp",
          "name": "metrics"
        }
      ],
      "secrets": [
        {
          "name": "TEMPORAL_OMES_AUTH_HEADER",
          "valueFrom": "arn:aws:secretsmanager:<REGION>:<ACCOUNT_ID>:secret:<SECRET_NAME>"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/sdk-perf-tests",
          "awslogs-region": "<REGION>",
          "awslogs-stream-prefix": "sdk-worker"
        }
      }
    }
  ]
}